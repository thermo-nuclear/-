#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i, j, n, m, c=0, k=1;
    scanf ("%d %d", &n, &m);
    int* live = (int*)malloc(n * sizeof(int));

    for (i = 0; i < n; i++)
    {
        live [i] = 1;
    }
    for (i = 0; i < n; i++)
    {
        for (j = 0; j < m; j++)
        {
            do
            {
                c = (c + 1) % n;
            }
            while (live [c] != 1);
        }
        live [c] = 0;
        if (i >= n - 3)
        {
            printf ("%d ", c);
        }
    }
    return 0;
}
