#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n, i, c=1;
    float r;
    scanf ("%d", &n);
    r = sqrt(n);
    for (i = 2; i <= r; i++)
    {
        if (n % i == 0)
        {
            c = 0;
            break;
        }
    }
    if (c == 0)
    {
        printf ("NO");
    }
    else
    {
        printf ("YES");
    }
}
