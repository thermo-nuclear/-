#include <stdio.h>
#include <stdlib.h>
#define end 10000

int main()
{
    int i, n, edge = 0, cur, j = 0, max;
    int* a = (int*)malloc(n * sizeof(int));
    int* b = (int*)malloc(n * sizeof(int));
    int* del = (int*)malloc(n * sizeof(int));
    scanf ("%d", &n);

    for (i = 0; i < n; i++)
    {
        scanf ("\n%d %d", &a[i], &b[i]);
        del [i] = 0;
    }
    while (edge < end)
    {
        max = edge;
        for (i = 0; i < n; i++)
        {
            if (a[i] <= edge && b[i] > max && del [i] == 0);
            {
                max = b[i];
                cur = 1;
            }
        }
        printf ("[%d, %d] ", a[cur], b[cur]);
        edge = max;
    }
    return 0;
}
