#include <stdio.h>
#include <stdlib.h>
#define end 10000

int main()
{
    int i, n, edge = 0, cur, max;
    scanf ("%d", &n);
    int* a = (int*)malloc(n * sizeof(int));
    int* b = (int*)malloc(n * sizeof(int));
    for (i = 0; i < n; i++)
    {
        scanf ("%d %d", &a[i], &b[i]);
    }
    while (edge < end)
    {
        max = edge;
        for (i = 0; i < n; i++)
        {
            if (a[i] <= edge && b[i] > max)
            {
                cur = i;
                max = b[i];
            }
        }
        printf ("[%d, %d] ", a[cur], b[cur]);
        edge = max;
    }
    return 0;
}
