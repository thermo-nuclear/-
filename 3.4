#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main ()
{
  int n, i, j;
  scanf ("%d", &n);
  int array [100][100];
  for (i = 0; i<=n; i++)
  {
    array [i][0] = 1;
    for (j=1; j<=n; j++)
    {
      array [i][j] = 0;
    }
  }

  for (i=1; i<=n; i++)
  {
    for (j=0; j<n; j++)
    {
      array [i][j]=array[i-1][j-1] + array [i-1][j];
    }
  }
  for  (i = 0; i<n; i++)
  {
    printf ("%d ", array[n][i]);
  }
  printf ("1\n");
}
