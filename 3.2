#include <stdio.h>
#include <stdlib.h>

int main()
{
    int f1, f2, s, i, n;
    scanf ("%d", &n);
    f1 = 1;
    f2 = 1;
    if (n == 1 || n == 2)
    {
        printf ("1");
    }
    else
    {
        for (i = 0; i < n - 2; i++)
        {
            s = f2;
            f2 = f1 + f2;
            f1 = s;
        }
        printf ("%d", f2);
    }
}
